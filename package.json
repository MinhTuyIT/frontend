{
  "version": "0.0.1",
  "private": true,
  "name": "frontend",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start:dev": "cp .env.development .env.local && expo start -c",
    "start:staging": "cp .env.staging .env.local && expo start -c",
    "start:prod": "cp .env.production .env.local && expo start -c",
    "web:local": "cp .env.localhost .env.local && expo start --web",
    "web:dev": "cp .env.development .env.local && expo start --web",
    "web:staging": "cp .env.staging .env.local && expo start --web",
    "web:prod": "cp .env.production .env.local && expo start --web",
    "build:dev-ios": "eas build -p ios --profile dev",
    "build:staging-ios": "eas build -p ios --profile staging",
    "build:prod-ios": "eas build -p ios --profile production",
    "build:dev-android": "eas build -p android --profile dev",
    "build:staging-android": "eas build -p android --profile staging",
    "build:prod-android": "eas build -p android --profile production",
    "prepare": "yarn husky install",
    "generate:types:local": "DOTENV_CONFIG_PATH=.env.localhost graphql-codegen -r dotenv/config --config codegen.ts",
    "generate:types:dev": "DOTENV_CONFIG_PATH=.env.development graphql-codegen -r dotenv/config --config codegen.ts",
    "generate:types:staging": "DOTENV_CONFIG_PATH=.env.staging graphql-codegen -r dotenv/config --config codegen.ts",
    "generate:types:prod": "DOTENV_CONFIG_PATH=.env.production graphql-codegen -r dotenv/config --config codegen.ts",
    "dev:instrumented": "INSTRUMENT_CODE=1 expo start --web --port 3000",
    "eas:build:local:dev": "cp .env.development .env.local && source .env.secrets && echo $SENTRY_AUTH_TOKEN && SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN eas build --profile dev --local",
    "cypress:open": "cypress open",
    "postinstall": "patch-package",
    "eject": "expo eject",
    "test": "node_modules/.bin/jest test/**/*.spec.js",
    "detox:ios:debug:build": "cp .env.development .env && detox build --configuration ios.sim.debug",
    "detox:ios:debug:test": "cp .env.development .env && detox test --configuration ios.sim.debug",
    "detox:android:debug:build": "cp .env.development .env && detox build --configuration android.emu.debug",
    "detox:android:debug:test": "cp .env.development .env && detox test --configuration android.emu.debug",
    "test:web:dev": "cp .env.development .env && playwright test"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@aws-sdk/client-cognito-identity-provider": "3.574.0",
