    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1",
    "node": ">= 18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "jest-expo"
  }
